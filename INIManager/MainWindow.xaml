        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:INIManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="1151.85" Width="2059.709">
    <DockPanel HorizontalAlignment="Left"  LastChildFill="False" VerticalAlignment="Top">
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_File">
                <MenuItem Header="Manage _New Document" Click="menuFileNew_Click"/>

                <MenuItem Header="Select _Managed Document" Click="menuFileSelect_Click"/>
            </MenuItem>
        </Menu>
        <TreeView DockPanel.Dock="Left">

        </TreeView>
        <TextBox Name="documentView" DockPanel.Dock="Right">

        </TextBox>

    </DockPanel>

</Window>
